{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "env": { "jest": true, "node": true, "browser": true },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "import/prefer-default-export": 0,
    "jsx-a11y/media-has-caption": 0
  }
}
